{
  "name": "nextjs",
  "private": true,
  "packageManager": "yarn@4.12.0",
  "workspaces": [
    "core",
    "server",
    "client"
  ],
  "scripts": {
    "set-version": "yarn set version latest",
    "upgrade": "yarn upgrade-interactive",
    "build": "turbo run build --concurrency=3",
    "dev": "turbo run dev --concurrency=4",
    "docker:pull": "docker compose pull",
    "docker:build-pull": "docker compose build --pull",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:start": "docker compose start",
    "docker:stop": "docker compose stop",
    "docker:restart": "docker compose restart",
    "docker:rm": "docker compose rm"
  },
  "devDependencies": {
    "turbo": "^2.6.1"
  }
}
